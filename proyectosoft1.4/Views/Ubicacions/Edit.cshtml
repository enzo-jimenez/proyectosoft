@model proyectosoft1._4.Models.Ubicacion

@{
    ViewData["Title"] = "Editar ubicacion";
}

<h1 class="mb-4">Editar ubicacion</h1>


<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="UbId" />
            <div class="form-group">
                <label asp-for="UbUbicacion" class="control-label"><h5>Ubicacion</h5></label>
                <input asp-for="UbUbicacion" class="form-control" />
                <span asp-validation-for="UbUbicacion" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Guardar" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Volver a la lista</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
